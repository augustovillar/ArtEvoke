aws_region      = "us-east-1"
project_name    = "artevoke"
instance_type   = "g4dn.xlarge"  # GPU instance: g4dn.xlarge (16GB VRAM), g4dn.2xlarge (16GB VRAM), g5.xlarge (24GB VRAM)
